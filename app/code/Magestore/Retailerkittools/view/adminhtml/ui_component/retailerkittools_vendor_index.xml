<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="context" xsi:type="configurableObject">
		<argument name="class" xsi:type="string">Magento\Framework\View\Element\UiComponent\Context</argument>
		<argument name="namespace" xsi:type="string">retailerkittools_vendor_index</argument>
	</argument>
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">retailerkittools_vendor_index.magestore_retailerkittools_vendor_grid_data_source</item>
			<item name="deps" xsi:type="string">retailerkittools_vendor_index.magestore_retailerkittools_vendor_grid_data_source</item>
		</item>
		<item name="spinner" xsi:type="string">magestore_retailerkittools_vendor_columns</item>
		<item name="buttons" xsi:type="array" >
            <item name="add" xsi:type="array">
                <item name="name" xsi:type="string">add</item>
                <item name="label" xsi:type="string" translate="true">Add New Vendor</item>
                <item name="class" xsi:type="string">primary</item>
                <item name="url" xsi:type="string">*/*/new</item>
            </item>
        </item>
	</argument>
	<dataSource name="magestore_retailerkittools_vendor_grid_data_source">
		<argument name="dataProvider" xsi:type="configurableObject">
			<argument name="class" xsi:type="string">Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider</argument>
			<argument name="name" xsi:type="string">magestore_retailerkittools_vendor_grid_data_source</argument>
			<argument name="primaryFieldName" xsi:type="string">vendor_id</argument>
			<argument name="requestFieldName" xsi:type="string">id</argument>
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="component" xsi:type="string">Magento_Ui/js/grid/provider</item>
					<item name="update_url" path="mui/index/render" xsi:type="url"/>
					<item name="storageConfig" xsi:type="array">
						<item name="indexField" xsi:type="string">vendor_id</item>
					</item>
				</item>
			</argument>
		</argument>
	</dataSource>
	<listingToolbar name="listing_top">
		<argument name="data" xsi:type="array">
			<item name="config" xsi:type="array">
				<item name="sticky" xsi:type="boolean">true</item>
			</item>
		</argument>
		<bookmark name="bookmark"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
		<massaction name="listing_massaction">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/tree-massactions</item>
                </item>
            </argument>
            <action name="delete">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="confirm" xsi:type="array">
                            <item name="title" xsi:type="string" translate="true">Delete vendor(s)</item>
                            <item name="message" xsi:type="string" translate="true">Delete selected vendor(s)?</item>
                        </item>
                        <item name="type" xsi:type="string">delete</item>
                        <item name="label" xsi:type="string" translate="true">Delete</item>
                        <item name="url" xsi:type="url" path="retailerkittools/vendor/massdelete"/>
                    </item>
                </argument>
            </action>
        </massaction>
	</listingToolbar>
	<columns name="magestore_retailerkittools_vendor_columns">
		<argument name="data" xsi:type="array"/>
		<selectionsColumn name="ids">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="indexField" xsi:type="string">vendor_id</item>
				</item>
			</argument>
		</selectionsColumn>
		<column name="vendor_id">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="sorting" xsi:type="string">asc</item>
					<item name="label" translate="true" xsi:type="string">ID</item>
				</item>
			</argument>
		</column>
		<column name="vendor_name">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">Name</item>
				</item>
			</argument>
		</column>
		<column name="vendor_email">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">Email</item>
				</item>
			</argument>
		</column>
		<column name="vendor_phone">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">Phone</item>
				</item>
			</argument>
		</column>
		<column name="vendor_address">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">Address</item>
				</item>
			</argument>
		</column>
		<column name="vendor_city">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">City</item>
				</item>
			</argument>
		</column>
		<column name="vendor_zipcode">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">Zip/Postal Code</item>
				</item>
			</argument>
		</column>
		<column name="vendor_state" class="Magestore\Retailerkittools\Ui\Component\Listing\Columns\State">
			<argument name="data" xsi:type="array" >
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">State/Province</item>
				</item>
			</argument>
		</column>
		<column name="vendor_country" class="Magestore\Retailerkittools\Ui\Component\Listing\Columns\Country">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">Country</item>
				</item>
			</argument>
		</column>
		<actionsColumn name="actions" class="Magestore\Retailerkittools\Ui\Component\Listing\Columns\VendorActions">
		    <argument name="data" xsi:type="array">
		        <item name="config" xsi:type="array">
		            <item name="indexField" xsi:type="string">vendor_id</item>
		            <item name="urlEntityParamName" xsi:type="string"></item>
		        </item>
		    </argument>
		</actionsColumn>
	</columns>
</listing>
